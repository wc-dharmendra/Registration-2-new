{
  "name": "registration-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "next build",
    "build:staging": "env-cmd -f .env.staging next build",
    "start:custom": "NODE_ENV=production forever start server.js",
    "stop:custom": "NODE_ENV=production forever stop server.js",
    "start": "next start",
    "start:dev": "node server.js",
    "deploy:dev": "next build && pm2 startOrRestart ecosystem.config.js --env dev",
    "deploy:staging": "next build && pm2 startOrRestart ecosystem.config.js --env staging",
    "deploy:prod": "next build && pm2 startOrRestart ecosystem.config.js --env prod",
    "start:instance": "npm install --force && pm2 startOrRestart ecosystem.config.js --env prod",
    "start:dev:pm2": "pm2 startOrRestart ecosystem.config.js --env dev",
    "lint": "next lint"
  },
  "dependencies": {
    "@react-google-maps/api": "2.19.2",
    "copy-to-clipboard": "3.3.3",
    "cors": "2.8.5",
    "crypto-js": "4.2.0",
    "cssnano": "6.0.1",
    "dayjs": "1.11.10",
    "dotenv": "16.4.5",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "fabric": "5.3.0",
    "forever": "4.0.3",
    "mongoose": "7.6.3",
    "next": "13.5.4",
    "next-themes": "0.2.1",
    "nookies": "2.5.2",
    "react": "18",
    "react-aria": "3.30.0",
    "react-beautiful-dnd": "13.1.1",
    "react-circular-progressbar": "2.1.0",
    "react-csv": "2.2.2",
    "react-dom": "18",
    "react-image-crop": "10.1.8",
    "react-quill": "2.0.0",
    "react-resizable-rotatable-draggable": "0.2.0",
    "react-rnd": "10.4.1",
    "react-share": "4.4.1",
    "react-stately": "3.28.0",
    "react-toastify": "9.1.3",
    "sharp": "0.33.1",
    "use-places-autocomplete": "4.0.1",
    "zustand": "4.4.3"
  },
  "devDependencies": {
    "autoprefixer": "10",
    "eslint": "8",
    "eslint-config-next": "13.5.4",
    "postcss": "8",
    "run": "1.4.0",
    "tailwindcss": "3"
  }
}
